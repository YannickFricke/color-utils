{
    "compilerOptions": {
        "target": "es2016",
        "lib": ["DOM"],
        "module": "commonjs",
        "rootDir": "./src/",
        "declaration": true,
        "outDir": "./dist/",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "exclude": ["./src/**/__tests__/*.ts"]
}
