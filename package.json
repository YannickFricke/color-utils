{
    "name": "@yannickfricke/color-utils",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "format:check": "prettier --check ./Readme.md ./package.json ./tsconfig.json",
        "format:write": "prettier --write ./Readme.md ./package.json ./tsconfig.json",
        "prepare": "husky install"
    },
    "devDependencies": {
        "husky": "^8.0.3",
        "lint-staged": "^13.2.3",
        "prettier": "^2.8.8",
        "typescript": "^5.1.6"
    },
    "lint-staged": {
        "*.{ts,md,json}": "prettier --write"
    }
}
